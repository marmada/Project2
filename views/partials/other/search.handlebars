/* Search Input area to look out food*/

<input type="search" autocomplete="off" tabindex="-1" aria-expanded="true" aria-label="Select box" aria-owns="ui-select-choices-0"
    aria-activedescendant="ui-select-choices-row-0-0" class="form-control ui-select-search ng-valid ng-dirty ng-touched" placeholder="Search Food Database"
    ng-model="$select.search" ng-show="$select.searchEnabled &amp;&amp; $select.open" style="width: 340px;">